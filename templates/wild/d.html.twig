{% extends 'base.html.twig' %}

{% block body %}
    <div class="container">
        <div class="row">
            {% for category in categories %}
                <div class="card h-100" >
                    <div class="card-body">
                        <h3 class="card-title">
                            {{ category.id }} - {{ category.name }}
                        </h3>
                    </div>
                </div>
            {% endfor %}
        </div>
    </div>

{% endblock %}